<a href="/teams">Team Index</a>
<a href="/players">Player Index</a>
<% @players.each do |player| %>
  <p><%= player.name %></p>
  <p><%= player.salary %></p>
  <p><%= player.position %></p>
  <p><%= player.injured %></p>
  <a href="/player_table_name/<%= player.id %>/edit">Edit <%= player.name %></a>
  <%= button_to "Delete #{player.name}", "/player_table_name/#{player.id}", method: :delete %>
<% end %>